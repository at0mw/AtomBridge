{
  "name": "atom-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "COMPLETEBUILD:onestepweb": "npm run build && npm run build:archive && npm run build:deployweb",
    "COMPLETEBUILD:onestepmobile": "npm run build && npm run build:archive && npm run build:deploymobile",
    "COMPLETEBUILD:onesteptouchpanel": "npm run build && npm run build:archive && npm run build:deploytouchpanel",
    "build:deployweb": "ch5-cli deploy -H 10.100.203.139 -p -t web dist/atom-ui.ch5z",
    "build:deploymobile": "ch5-cli deploy -H 10.100.203.139 -p -t mobile dist/atom-ui.ch5z",
    "build:deploytouchpanel": "ch5-cli deploy -H 192.168.1.122 -p -t touchscreen dist/atom-ui.ch5z",
    "build:archive": "npm run edit:index && ch5-cli archive -p atom-ui -d dist/atom-ui -o dist",
    "edit:index": "node scripts/edit-index-post-build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@primeng/themes": "^19.0.10",
    "primeng": "^19.0.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
